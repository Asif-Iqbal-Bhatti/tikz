{
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "description": "SvelteKit gallery site of standalone TikZ images along with their LaTeX source code",
  "homepage": "https://tikz.netlify.app",
  "license": "MIT",
  "name": "tikz",
  "repository": "https://github.com/janosh/tikz",
  "type": "module",
  "version": "1.0.0",
  "bugs": {
    "url": "https://github.com/janosh/tikz/issues"
  },
  "scripts": {
    "build": "svelte-kit build",
    "dev": "svelte-kit dev",
    "preview": "svelte-kit preview",
    "serve": "yarn build && yarn preview",
    "prebuild": "yarn symlinkAssets && yarn predev",
    "predev": "node src/parseTexFiles.js",
    "symlinkAssets": "cd static && ln -fs ../../assets . && cd -",
    "check": "svelte-check"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.23",
    "@sveltejs/kit": "^1.0.0-next.206",
    "@svicons/fa-regular": "^0.1.6",
    "@svicons/octicons": "^0.1.6",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "image-size": "^1.0.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "svelte": "^3.44.3",
    "svelte-bricks": "^0.1.3",
    "svelte-check": "^2.2.11",
    "svelte-preprocess": "^4.10.1",
    "svelte2tsx": "^0.4.12",
    "typescript": "^4.5.4",
    "vite": "^2.7.10"
  }
}
