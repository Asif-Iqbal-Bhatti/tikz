{
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "description": "SvelteKit site displaying standalone TikZ images along with their LaTeX source code",
  "homepage": "https://tikz.netlify.app",
  "license": "MIT",
  "name": "tikz",
  "repository": "https://github.com/janosh/tikz",
  "type": "module",
  "version": "1.0.0",
  "bugs": {
    "url": "https://github.com/janosh/tikz/issues"
  },
  "scripts": {
    "build": "svelte-kit build",
    "dev": "svelte-kit dev",
    "preview": "svelte-kit preview",
    "serve": "yarn build && yarn preview",
    "prebuild": "yarn symlinkAssets && yarn predev",
    "predev": "node src/parseTexFiles.js",
    "symlinkAssets": "cd static && ln -fs ../../assets . && cd -",
    "check": "svelte-check"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.20",
    "@sveltejs/kit": "^1.0.0-next.179",
    "@svicons/fa-regular": "^0.1.3",
    "@svicons/octicons": "^0.1.3",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "image-size": "^1.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.43.1",
    "svelte-bricks": "^0.1.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.8",
    "svelte2tsx": "^0.4.7",
    "typescript": "^4.4.3",
    "vite": "^2.6.2"
  }
}
