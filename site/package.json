{
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "description": "SvelteKit site displaying standalone TikZ images along with their LaTeX source code",
  "homepage": "https://tikz.netlify.app/",
  "license": "MIT",
  "name": "tikz",
  "repository": "https://github.com/janosh/tikz",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "build": "svelte-kit build",
    "dev": "svelte-kit dev",
    "preview": "svelte-kit preview",
    "serve": "yarn build && yarn preview",
    "prebuild": "yarn symlinkAssets && yarn predev",
    "predev": "node src/parseTexFiles.js",
    "symlinkAssets": "ln -sf $PWD/../assets $PWD/static",
    "check": "svelte-check --workspace src --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.13",
    "@sveltejs/kit": "^1.0.0-next.132",
    "@svicons/fa-regular": "^0.1.3",
    "@svicons/octicons": "^0.1.3",
    "@tsconfig/svelte": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "image-size": "^1.0.0",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "svelte": "^3.38.3",
    "svelte-bricks": "^0.1.0",
    "svelte-check": "^2.2.3",
    "svelte-preprocess": "^4.7.4",
    "svelte2tsx": "^0.4.2",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "vite": "^2.4.3"
  }
}
