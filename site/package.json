{
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "description": "svelte-kit site displaying TikZ images along with LaTeX source code",
  "devDependencies": {
    "@sveltejs/adapter-netlify": "^1.0.0-next.4",
    "@sveltejs/kit": "^1.0.0-next.64",
    "eslint": "^7.22.0",
    "eslint-plugin-svelte3": "^3.1.2",
    "image-size": "^0.9.7",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "svelte": "^3.35.0",
    "vite": "^2.1.4"
  },
  "homepage": "https://github.com/janosh/tikz#readme",
  "license": "MIT",
  "name": "tikz",
  "repository": "https://github.com/janosh/tikz",
  "scripts": {
    "build": "svelte-kit build",
    "dev": "svelte-kit dev",
    "prebuild": "yarn symlinkAssets && node src/parseTexFiles.js",
    "predev": "node src/parseTexFiles.js",
    "serve": "svelte-kit build && svelte-kit start",
    "start": "svelte-kit start",
    "symlinkAssets": "ln -sf $PWD/../assets $PWD/static"
  },
  "type": "module",
  "version": "1.0.0"
