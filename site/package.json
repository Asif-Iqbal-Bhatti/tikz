{
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "description": "SvelteKit gallery site of standalone TikZ images along with their LaTeX source code",
  "homepage": "https://tikz.netlify.app",
  "license": "MIT",
  "name": "tikz",
  "repository": "https://github.com/janosh/tikz",
  "type": "module",
  "version": "1.0.0",
  "bugs": "https://github.com/janosh/tikz/issues",
  "scripts": {
    "build": "svelte-kit build",
    "dev": "svelte-kit dev",
    "preview": "svelte-kit preview",
    "serve": "yarn build && yarn preview",
    "prebuild": "node src/parse-assets.js",
    "predev": "node src/parse-assets.js && yarn symlink",
    "symlink": "cd static && ln -fs ../../assets . && cd -",
    "check": "svelte-check"
  },
  "devDependencies": {
    "@iconify-json/octicon": "^1.1.10",
    "@sveltejs/adapter-static": "^1.0.0-next.32",
    "@sveltejs/kit": "^1.0.0-next.345",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "image-size": "^1.0.1",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "rehype-katex": "^6.0.2",
    "rehype-stringify": "^9.0.3",
    "remark-math": "^5.1.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.1",
    "svelte-github-corner": "^0.1.0",
    "svelte-multiselect": "^5.0.1",
    "svelte-preprocess": "^4.10.6",
    "svelte2tsx": "^0.5.10",
    "typescript": "^4.7.2",
    "unified": "^10.1.2",
    "unplugin-icons": "^0.14.3",
    "vite": "^2.9.9"
  }
}
